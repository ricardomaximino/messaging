package com.brasajava.rabbitstreamreactiveone.domain;

import java.time.LocalDateTime;

public class ContactEvent {

	private String id;
	private String type;
	private String description;
	private String contactId;
	private Contact contact;
	private LocalDateTime sendDate;
	private LocalDateTime receiveDate;
	
	
	
	public ContactEvent() {	}
	public ContactEvent(String type, String description, String contactId, Contact contact, LocalDateTime sendDate) {
		super();
		this.type = type;
		this.description = description;
		this.contactId = contactId;
		this.contact = contact;
		this.sendDate = sendDate;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getContactId() {
		return contactId;
	}
	public void setContactId(String contactId) {
		this.contactId = contactId;
	}
	public Contact getContact() {
		return contact;
	}
	public void setContact(Contact contact) {
		this.contact = contact;
	}
	public LocalDateTime getSendDate() {
		return sendDate;
	}
	public void setSendDate(LocalDateTime sendDate) {
		this.sendDate = sendDate;
	}
	public LocalDateTime getReceiveDate() {
		return receiveDate;
	}
	public void setReceiveDate(LocalDateTime receiveDate) {
		this.receiveDate = receiveDate;
	}
	@Override
	public String toString() {
		return "ContactEvent [id=" + id + ", type=" + type + ", description=" + description + ", contactId=" + contactId
				+ ", contact=" + contact + ", sendDate=" + sendDate + ", receiveDate=" + receiveDate + "]";
	}

}
